        <StackPanel Grid.Row="0" HorizontalAlignment="Left" Orientation="Horizontal">
            <Button Margin="2" Click="Button_Click">
                <TextBlock Margin="2" >Kartadan o'qish</TextBlock>
            </Button>
        </StackPanel>
        <StackPanel Grid.Row="1" Name="DynPanel">

        </StackPanel>
    </Grid>
</UserControl>
