<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:GlobalSolutions"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <FontFamily x:Key="UbuntuRegular">pack://application;,,,/Fonts/#Ubuntu Regular</FontFamily>
    <FontFamily x:Key="UbuntuBold">pack://application;,,,/Fonts/#Ubuntu Bold</FontFamily>
    <FontFamily x:Key="UbuntuItalic">pack://application;,,,/Fonts/#Ubuntu Italic</FontFamily>
    <FontFamily x:Key="FontAwesome">pack://application;,,,/Fonts/#FontAwesome</FontFamily>
    
    <system:Double x:Key="FontSizeRegular">15</system:Double>
    <system:Double x:Key="FontSizeLarge">20</system:Double>
    <system:Double x:Key="FontSizeSmall">10</system:Double>
</ResourceDictionary>